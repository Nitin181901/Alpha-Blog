<h1> Listing page </h1>
<table>
   <thead>
      AlphaBlog
      <tr>
         <th>Title</th>
         <th>Description</th>
         <th>actions</th>
      </tr>
   </thead>
   <tbody>
      <% @articles.each do |article| %>
      <tr>
         <td><%= article.title %></td>
         <td><%= article.description %></td>
         <td><%= link_to 'Delete', article_path(article), method: :delete  %></td>
      </tr>
      <% end %>
   </tbody>
</table>
